{
    "name": "soluzione-software/laravel-translations",
    "description": "The package adds extra functionalities to Laravel's localization system.",
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "illuminate/config": "5.8.*",
        "illuminate/console": "5.8.*",
        "illuminate/container": "5.8.*",
        "illuminate/filesystem": "5.8.*",
        "illuminate/support": "5.8.*",
        "illuminate/translation": "5.8.*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SoluzioneSoftware\\LaravelTranslations\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoluzioneSoftware\\LaravelTranslations\\ServiceProvider"
            ]
        }
    }
}
